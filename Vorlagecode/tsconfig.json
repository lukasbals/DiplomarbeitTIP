{
    "version": "1.5.0-beta",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./**/*.ts",
        "./**/*.tsx",
        "!./node_modules/**/*"
    ],
    "files": [
        "./app.ts",
        "./public/javascripts/Initialize.ts",
        "./public/javascripts/controllers/TestCtrl.ts",
        "./public/javascripts/services/TestService.ts",
        "./routes/api.ts",
        "./typings/angularjs/angular.d.ts",
        "./typings/body-parser/body-parser.d.ts",
        "./typings/cookie-parser/cookie-parser.d.ts",
        "./typings/devextreme/dx.devextreme.d.ts",
        "./typings/express/express.d.ts",
        "./typings/form-data/form-data.d.ts",
        "./typings/globalize/globalize.d.ts",
        "./typings/jquery/jquery.d.ts",
        "./typings/mime/mime.d.ts",
        "./typings/morgan/morgan.d.ts",
        "./typings/node/node.d.ts",
        "./typings/request/request.d.ts",
        "./typings/serve-favicon/serve-favicon.d.ts",
        "./typings/serve-static/serve-static.d.ts",
        "./typings/sqlite3/sqlite3.d.ts",
        "./typings/tsd.d.ts"
    ]
}
